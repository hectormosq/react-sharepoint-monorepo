{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "no-console": false,
    "no-implicit-dependencies": false,
    "jsx-wrap-multiline": true,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "no-var-requires": false,
    "ordered-imports": true,
    "quotemark": [true, "single"]
  },
  "linterOptions": {
    "exclude": [
      "dist",
      "node_modules"
    ]
  }
}
